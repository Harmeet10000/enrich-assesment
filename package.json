{
  "name": "enrich-assessment",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "engines": {
    "node": ">=22.14.0",
    "npm": ">=10.7.0"
  },
  "scripts": {
    "dev": "nodemon  src/index.js",
    "build": "rm -rf dist && npx webpack --config webpack.config.js",
    "loadtest": "k6 run k6_load_test.js",
    "start": "node dist/index.cjs",
    "swagger": "node ./docs/swagger.js",
    "test": "node test-runner.js",
    "test:watch": "node --watch test-runner.js",
    "test:coverage": "node --test --coverage test-runner.js",
    "prepare": "husky install",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "migrate:dev": "cross-env MIGRATE_MODE=development node script/migration.js",
    "migrate:prod": "cross-env MIGRATE_MODE=production node script/migration.js",
    "backup:start": "node scripts/dbBackup.js",
    "backup:once": "cross-env RUN_BACKUP_ONCE=true node scripts/dbBackup.js"
  },
  "keywords": [],
  "author": "Harmeet Singh",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Harmeet10000/ts-backend-production-template.git"
  },
  "bugs": {
    "url": "https://github.com/Harmeet10000/ts-backend-production-template/issues"
  },
  "homepage": "https://github.com/Harmeet10000/ts-backend-production-template#readme",
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "npm run format:fix"
    ]
  },
  "description": "",
  "devDependencies": {
    "@babel/cli": "^7.27.0",
    "@babel/core": "^7.26.10",
    "@babel/node": "^7.26.0",
    "@babel/plugin-transform-class-properties": "^7.25.9",
    "@babel/plugin-transform-object-rest-spread": "^7.25.9",
    "@babel/plugin-transform-runtime": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/runtime": "^7.27.0",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@eslint/js": "^9.24.0",
    "babel-loader": "^10.0.0",
    "dotenv-webpack": "^8.1.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "husky": "9.1.7",
    "lint-staged": "^15.4.3",
    "nodemon": "^3.1.9",
    "prettier": "3.5.2",
    "supertest": "^7.1.0",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "bullmq": "5.56.0",
    "colorette": "^2.0.20",
    "compression": "1.8.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "16.5.0",
    "dotenv-flow": "^4.1.0",
    "express": "^4.21.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-prom-bundle": "8.0.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "hpp": "0.2.3",
    "ioredis": "5.6.1",
    "joi": "^17.13.3",
    "mongoose": "^8.10.1",
    "nanoid": "5.1.5",
    "node-cron": "^3.0.3",
    "prom-client": "15.1.3",
    "rate-limit-redis": "4.2.1",
    "swagger-autogen": "^2.23.7",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-mongodb": "^6.0.0",
    "xss-clean": "0.1.4"
  },
  "config": {
    "unsafe-perm": true
  }
